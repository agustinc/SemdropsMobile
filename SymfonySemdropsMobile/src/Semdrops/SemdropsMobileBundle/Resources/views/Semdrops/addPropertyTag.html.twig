<link href="{{ asset('iphone.css') }}" rel="stylesheet" type="text/css" />
{% block head %}
<HEAD>
<TITLE>Property Tag - Semdrops Mobile</TITLE>
</HEAD> 
{% endblock %}
{% block title %} Semdrops Mobile - Add Property Tag {% endblock %}
<form action="{{ path('donePropertyTag') }}" method="POST" {{form_enctype(form)}}>
   <br>
   {{form_label(form.uri,'Here go your uri: ')}} 
   <br>
   {{form_widget(form.uri)}}
   <br>
   {{form_label(form.propertyTag,'Here go your Property Tag: ')}}
   <br>
   {{form_widget(form.propertyTag)}}
   <br>
   {{form_label(form.destino,'Here go your Destino: ')}}
   <br>
   {{form_widget(form.destino)}}
   <br>
   <input type="submit" value ="Add"/>
   <input type="reset" value="Clear"><br><br>
</form>

