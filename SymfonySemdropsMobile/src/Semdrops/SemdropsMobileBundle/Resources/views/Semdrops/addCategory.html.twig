<link href="{{ asset('iphone.css') }}" rel="stylesheet" type="text/css" />
{% block head %}
<HEAD>
<TITLE>Category - Semdrops Mobile</TITLE>
</HEAD> 
{% endblock %}
{% block title %} Semdrops Mobile - Add Category{% endblock %}<br>
<form action="{{ path('doneCategory') }}" method="POST" {{form_enctype(form)}}>
   <br>
